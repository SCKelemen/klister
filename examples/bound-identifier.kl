#lang kernel

(import (shift kernel 1))

(define-macros
  ((m (lambda (stx)
        (syntax-case stx
          ((list (_ x y))
           (pure (if (bound-identifier=? x y)
                     ''t
                     ''f))))))))

(example (bound-identifier=? 'x 'x))
(example (bound-identifier=? 'x 'y))
(example (m x x))
(example (m x y))
